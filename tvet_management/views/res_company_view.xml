<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="action_view_company_compnay" model="ir.ui.view">
            <field name="name">res.company.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='currency_id']" position="after">
                      <field name="at_dance"/>
                </xpath>
                <xpath expr="//field[@name='favicon']" position="after">
                    <field name="student_card_sign" widget="image" class="float-left oe_avatar"/>
                    <field name="uni_header" widget="image" class="float-left oe_avatar"/>
                    <field name="active_company"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
